{
  "ConnectionStrings": {
    "ClickHouse": "Host=localhost;Database=external_dictionary_sample"
  },
  "ExternalConnections": {
    "PostgresMain": {
      "HostPort": "localhost:5432",
      "Database": "mydb",
      "User": "postgres",
      "PasswordEnv": "PG_PASSWORD"
    },
    "PostgresReplica": {
      "HostPort": "pg-replica:5432",
      "Database": "mydb",
      "UserEnv": "PG_REPLICA_USER",
      "PasswordEnv": "PG_REPLICA_PASSWORD"
    },
    "MySqlInventory": {
      "HostPort": "localhost:3306",
      "Database": "inventory",
      "User": "root",
      "PasswordEnv": "MYSQL_PASSWORD"
    },
    "ExchangeRateApi": {
      "Url": "https://api.exchangerate.example.com/latest",
      "UserEnv": "EXCHANGE_API_USER",
      "PasswordEnv": "EXCHANGE_API_KEY"
    }
  }
}
