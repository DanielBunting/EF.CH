<?xml version="1.0"?>
<!--
  User configuration for all ClickHouse nodes
  This file is shared across all nodes in the cluster
-->
<clickhouse>
    <users>
        <default>
            <password></password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </default>
    </users>
</clickhouse>
